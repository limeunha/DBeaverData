-- 1. insert: 테이블에 데이터 추가
INSERT INTO CUSTOMER(ID , NAME, ADDRESS)
VALUES(1, '둘리', '인천');

--모든 컬럼의 데이터를 넣을때는 컬럼명 생략가능(순서는 create table 했을때 지정한 컬럼 순서)
INSERT INTO CUSTOMER VALUES(2, '둘리', '인천');

SELECT * FROM CUSTOMER;

--불완전한 데이터 추가(컬럼명을 반드시 넣어준다)
INSERT INTO CUSTOMER(ID, NAME) VALUES(3,'또치');

--컬럼명을 쓰고 싶지 않을때
INSERT INTO CUSTOMER VALUES(4,'둘리','');
INSERT INTO CUSTOMER VALUES(5,'홍길동', null);

-- primary key에 컬럼 값이 중복될때
INSERT INTO CUSTOMER(ID,NAME) VALUES(3,'홍길동');

--not null 컬럼에 반값으로 데이터 추가하기
SELECT INTO CUSTOMER(ID) VALUES(6);

INSERT INTO ORDERS(ORDER_ID,SALESMAN_ID,ORDER_DATE)
VALUES(1001, 2002, sysdate);

INSERT INTO ORDERS(ORDER_ID,SALESMAN_ID,ORDER_DATE)
VALUES(1002, 2002, '2022-08-27');

INSERT INTO ORDERS(ORDER_ID,SALESMAN_ID,ORDER_DATE)
VALUES(1003, 2002, '2022-08-27 11:11:11');

-- 특정 포맷으로 날짜 데이터 추가
INSERT INTO ORDERS
VALUES(1003, 2002, to_date('2022-08-27','YYYY-MM-DD'));

INSERT INTO ORDERS
VALUES(1004, 2002, to_date('2022-08-27' 10:10;10,'YYYY-MM-DD HH24:MI:SS'));

SELECT * FROM ORDERS;

